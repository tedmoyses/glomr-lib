{
    "name": "tedmoyses/glomr-lib",
    "description": "The library implemenation of Glomr the static stite builder",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Ted Moyses",
        "role": "developer"
      }
    ],
    "require": {
        "jenssegers/blade": "^1.1",
        "monolog/monolog": "^1.23",
        "bramus/monolog-colored-line-formatter": "~2.0",
        "symfony/process": "~3.0",
        "kriswallsmith/assetic": "^1.4",
        "patchwork/jsqueeze": "^2.0",
        "matthiasmullie/minify": "^1.3",
        "tedivm/jshrink": "^1.3",
        "illuminate/filesystem": "^5.7",
        "symfony/finder": "^4.1",
        "league/flysystem": "^1.0"
    },
    "autoload": {
      "psr-4": {"Glomr\\": "src/"}
    },
    "autoload-dev":{
      "psr-4": {"Glomr\\Test\\": "tests/"}
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
